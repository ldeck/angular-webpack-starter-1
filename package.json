{
  "name": "angular-webpack-starter-kit",
  "version": "5.2.1",
  "description": "An Angular Webpack Starter kit featuring Angular 1.x (ui-router, Tests, E2E, Dev/Prod), Karma, Protractor, Jasmine, Cucumber, Istanbul, Babel, Postcss, Jade, livereload and Webpack",
  "keywords": [
    "angular",
    "webpack",
    "babel",
    "postcss",
    "jade",
    "livereload",
    "karma",
    "protractor",
    "bdd",
    "cucumber",
    "gherkin",
    "jasmine",
    "Istanbul",
    "workflow",
    "e2e",
    "starter",
    "kit",
    "starter kit"
  ],
  "scripts": {
    "build": "./node_modules/.bin/rimraf dist && webpack --bail --progress --profile",
    "server": "./node_modules/.bin/webpack-dev-server --history-api-fallback --inline --progress",
    "start": "npm run server",
    "stats": "./node_modules/.bin/webpack --json --profile > stats.json",

    "test-all": "npm test && npm run e2e && npm run bdd",
    "test": "./node_modules/.bin/karma start",
    "test-watch": "./node_modules/.bin/karma start --auto-watch --no-single-run",
    "e2e": "./node_modules/.bin/protractor protractor.jasmine.conf.js",
    "e2e-watch": "npm run e2e -- --elementExplorer",
    "bdd": "./node_modules/.bin/protractor protractor.bdd.conf.js",
    "bdd-wip": "npm run bdd -- --cucumberOpts.tags @wip",
    "bdd-wip-watch": "./node_modules/.bin/watchy -w features,src -- bin/queuebdd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ldeck/angular-webpack-starter-kit.git"
  },
  "author": "ldeck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ldeck/angular-webpack-starter-kit/issues"
  },
  "homepage": "https://github.com/ldeck/angular-webpack-starter-kit",
  "dependencies": {
    "angular": "^1.5.0",
    "angular-ui-router": "^0.2.18",
    "angular-ui-router.statehelper": "^1.3.1",
    "fastclick": "^1.0.6"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "css-mqpacker": "^4.0.0",
    "cucumber": "^0.10.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.7.1",
    "isparta-instrumenter-loader": "^1.0.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.14",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "^1.7.0",
    "ng-annotate-loader": "^0.1.0",
    "node-libs-browser": "^1.0.0",
    "null-loader": "^0.1.1",
    "postcss-discard-comments": "^2.0.4",
    "postcss-functions": "^2.1.0",
    "postcss-hexrgba": "^0.2.0",
    "postcss-import": "^8.0.2",
    "postcss-inline-comment": "^2.0.0",
    "postcss-loader": "^0.8.0",
    "postcss-size": "^1.0.0",
    "precss": "^1.4.0",
    "protractor": "^3.1.1",
    "protractor-cucumber-framework": "^0.4.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
